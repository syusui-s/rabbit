{
  "name": "rabbit",
  "version": "0.0.0",
  "description": "",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run generatePackageInfo && vite",
    "dev": "npm run generatePackageInfo && vite",
    "build": "npm run generatePackageInfo && vite build",
    "serve": "npm run generatePackageInfo && vite preview",
    "lint": "npm run prettier && npm run eslint",
    "fix": "npm run prettier-fix && npm run eslint-fix",
    "eslint": "eslint --cache .",
    "prettier": "prettier -c .",
    "eslint-fix": "eslint --cache --fix .",
    "prettier-fix": "prettier --write .",
    "tsc": "tsc --noEmit --skipLibCheck",
    "test": "vitest run --no-watch",
    "watch-test": "vitest --watch",
    "cover": "vitest run --coverage",
    "prepare": "husky install",
    "generatePackageInfo": "node -e \"import('./scripts/generatePackageInfo.mjs').then((m) => m.default())\"",
    "checkLicense": "node -e \"import('./scripts/checkLicense.mjs').then((m) => m.default())\"",
    "checkSecrets": "node -e \"import('./scripts/checkSecrets.mjs').then((m) => m.default())\""
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.13",
    "@types/jsdom": "^21.1.7",
    "@types/qrcode": "^1.5.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-solid": "^2.11.8",
    "vite-plugin-solid-svg": "^0.8.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/query-async-storage-persister": "^5.85.5",
    "@tanstack/query-persist-client-core": "^5.85.5",
    "@tanstack/solid-query": "^5.85.5",
    "@textcomplete/core": "^0.1.13",
    "@textcomplete/textarea": "^0.1.13",
    "@thisbeyond/solid-dnd": "^0.7.5",
    "@types/lodash": "^4.17.20",
    "@types/ua-parser-js": "^0.7.39",
    "bech32": "^2.0.0",
    "bolt11": "^1.4.1",
    "emoji-mart": "^5.6.0",
    "heroicons": "^2.2.0",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "idb-keyval": "^6.2.2",
    "lodash": "^4.17.21",
    "nostr-tools": "2.10.1",
    "nostr-wasm": "^0.1.0",
    "qrcode": "^1.5.4",
    "solid-js": "^1.9.9",
    "tailwindcss": "^4.1.12",
    "ua-parser-js": "^1.0.39",
    "webln": "^0.3.2",
    "zod": "^4.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,json}": "./scripts/fix.sh"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "sourceMap": true,
    "all": true
  }
}
